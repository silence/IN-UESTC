/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.page {
  background: #fff;
}

@font-face {
  font-family: 'iconfont';  /* project id 919313 */
  src: url('//at.alicdn.com/t/font_919313_f59nsph5oh.eot');
  src: url('//at.alicdn.com/t/font_919313_f59nsph5oh.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_919313_f59nsph5oh.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_919313_f59nsph5oh.woff') format('woff'),
  url('//at.alicdn.com/t/font_919313_f59nsph5oh.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_919313_f59nsph5oh.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-cycle::before {
  content: "\e606";
}

.icon-pos::before {
  content: "\e614";
}

.icon-issue::before {
  content: "\e69e";
}

.icon-message::before {
  content: "\e609";
}

.icon-arrow-right::before {
  content: "\e6a1";
}

.icon-profile::before {
  content: "\e64c";
}

